{
  "name": "kareljs",
  "version": "1.0.0",
  "description": "Implementation of Karel the Robot in JavaScript",
  "main": "index.js",
  "scripts": {
    "test": "tape tests/test_api.js | tap-spec && mocha tests/testKarelInterface.js && node tests/test_models.js && node ./tests/validate_levels.js ../public/levels.json",
    "start": "node src/server.js",
    "build": "npx webpack-cli --entry ./src/index.js --output public/appbundle.js",
    "watch": "watchify -t [ babelify --presets [ es2015 react ] ] ./src/index.js -o ./public/appbundle.js"
  },
  "author": "Chris Tsoufakis",
  "license": "MIT",
  "repository": "https://github.com/tsoufakis/kareljs",
