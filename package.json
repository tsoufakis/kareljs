    "browserify": "^16.5.1",
    "chai": "^4.2.0",
    "codemirror": "^5.52.2",
    "es6-promise": "^4.2.8",
    "esprima": "^4.0.1",
    "express": "^4.17.1",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.5.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "lodash.eq": "^4.0.0",
    "mongoose": "^5.9.7",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.2",
    "prop-types": "^15.7.2",
